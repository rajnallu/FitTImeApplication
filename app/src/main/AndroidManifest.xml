<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fittimeapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FitTimeApplication">
        <activity android:name=".StrengthResistanceExercise"></activity>
        <activity android:name=".FlexibilityExercise" />
        <activity android:name=".AerobicExercise" />
        <activity android:name=".ExcercisePage" />
        <activity android:name=".ScreenTimeLimits" />
        <activity android:name=".GoalsPage" />
        <activity android:name=".SurveyPage2" />
        <activity android:name=".surveryPage1" />
        <activity android:name=".LoginActivity2" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>